generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = "mongodb://127.0.0.1:27017/restaurant"
}

model restaurants {
  id            String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  address       Json
  borough       String
  cuisine       String
  grades        Json[]
  name          String
  restaurant_id String
}
